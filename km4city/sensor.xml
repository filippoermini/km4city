<?xml version="1.0"?>
<tree typeId="http://www.w3.org/1999/02/22-rdf-syntax-ns#type" isStateful="false">
    <classIterationQuery>
    	<server>http://servicemap.disit.org/WebAppGrafo/sparql</server>
    	<query>SELECT DISTINCT (substr(str(?s),39) as ?id) WHERE {?s a km4c:Charging_stations.} limit 10</query>
    </classIterationQuery>
    <fileInfo>
    	<fileName>recharge.n3</fileName>
    	<startDirectory>./recharge/</startDirectory>
    </fileInfo>
    <iterationElement>
    	<class type="http://www.disit.org/km4city/schema#Charging_Stations" name="Charging_Station" isRoot="true" baseUri="http://www.disit.org/km4city/resource">
	        <properties>
	            <prop key="http://purl.org/dc/terms/identifier">
	                <type>id</type>
	                <name>Charging_Stations</name>
	            </prop>
	            <prop key="http://www.w3.org/ns/ssn/hasSensor" isHidden="true">
	                <name>sensor_code</name>
	                <type>md5</type>
	                <md5String>ciaociao</md5String>
	            </prop>
	            <prop key="http://www.w3.org/ns/ssn/hasSensor">
	                <type>Sensor2</type>
	                <uri>http://www.w3.org/ns/ssn/Sensor</uri>
	            </prop>
	        </properties>
	    </class>
	    <class type="http://www.w3.org/ns/ssn/Sensor" name="Sensor1" isRoot="false" baseUri="http://www.disit.org/km4city/resource">
	        <properties>
	            <prop key="http://purl.org/dc/terms/identifier">
	                <type>valueExpression</type>
	                <valueExpression>${Charging_Stations} + "_sensor1" + ${sensor_code}</valueExpression>
	            </prop>
	            <prop key="http://www.w3.org/ns/ssn/MadeObservation">
	                <type>Observation1</type>
	            </prop>
	        </properties>
	    </class>
	    <class type="http://www.w3.org/ns/ssn/Sensor" name="Sensor2" isRoot="false" baseUri="http://www.disit.org/km4city/resource">
	        <properties>
	            <prop key="http://purl.org/dc/terms/identifier">
	                <type>valueExpression</type>
	                <valueExpression>${Charging_Station} + "_sensor2"</valueExpression>
	            </prop>
	            <prop key="http://www.w3.org/ns/ssn/MadeObservation">
	                <type>query</type>
	                <queryInfo>
	                	<server>http://servicemap.disit.org/WebAppGrafo/sparql</server>
				    	<query>SELECT DISTINCT (substr(str(?s),39) as ?id) WHERE {?s a km4c:Charging_stations.} limit 1</query>
				    	<bindingValue>id</bindingValue>
				    </queryInfo>
	            </prop>
	        </properties>
	    </class>
	    <class type="http://www.w3.org/ns/ssn/Observation" name="Observation1" isRoot="false" baseUri="http://www.disit.org/km4city/resource">
	        <properties>
	            <prop key="http://purl.org/dc/terms/identifier">
	                <type>UUID</type>
	            </prop>
	            <prop key="http://www.w3.org/ns/ssn/observedProperty" isUri="true">
					<type>FromSet</type>
	                <fromSet>http://www.disit.org/km4city/schema#chargeState</fromSet>
	            </prop>
	        </properties>
	    </class>
		<class type="http://www.w3.org/ns/ssn/SensorOutput" name="SensorOutput1" isRoot="false" baseUri="http://www.disit.org/km4city/resource">
	        <properties>
	            <prop key="http://purl.org/dc/terms/identifier">
	                <type>UUID</type>
	            </prop>
	            <prop key="http://www.w3.org/ns/ssn/observationResultTime">
	                <type>DateTime</type>
	                <uri>http://www.w3.org/2001/XMLSchema#dateTime</uri>
	            </prop>
				<prop key="http://www.w3.org/ns/ssn/hasValue">
					<type>ObservationValue1</type>
				</prop>
			</properties>
		</class>
		<class type="http://www.w3.org/ns/ssn/ObservationValue" name="ObservationValue1" isRoot="false" baseUri="http://www.disit.org/km4city/resource">
	        <properties>
	            <prop key="http://purl.org/dc/terms/identifier">
	                <type>UUID</type>
	            </prop>
	            <prop key="http://www.disit.org/km4city/schema#chargeState">
	                <name>chargeState1</name>
	                <type>Float</type>
	                <maxValue>3</maxValue>
	                <minValue>2</minValue>
	                <uri>http://www.w3.org/2001/XMLSchema#float</uri>
	            </prop>
			</properties>
		</class>
	    <class type="http://www.w3.org/ns/ssn/Observation" name="Observation2" isRoot="false" baseUri="http://www.disit.org/km4city/resource">
	        <properties>
	            <prop key="http://purl.org/dc/terms/identifier">
	                <type>UUID</type>
	            </prop>
	            <prop key="http://www.w3.org/ns/ssn/observedProperty">
					<type>FromSet</type>
	                <fromSet>http://www.disit.org/km4city/schema#lastChargeState</fromSet>
	            </prop>
	        </properties>
	    </class>
		<class type="http://www.w3.org/ns/ssn/SensorOutput" name="SensorOutput2" isRoot="false" baseUri="http://www.disit.org/km4city/resource">
	        <properties>
	            <prop key="http://purl.org/dc/terms/identifier">
	                <type>UUID</type>
	            </prop>
	            <prop key="http://www.w3.org/ns/ssn/observationResultTime">
	                <type>DateTime</type>
	                <uri>http://www.w3.org/2001/XMLSchema#dateTime</uri>
	            </prop>
				<prop key="http://www.w3.org/ns/ssn/hasValue">
					<type>ObservationValue2</type>
				</prop>
			</properties>
		</class>
		<class type="http://www.w3.org/ns/ssn/ObservationValue" name="ObservationValue2" isRoot="false" baseUri="http://www.disit.org/km4city/resource">
	        <properties>
	            <prop key="http://purl.org/dc/terms/identifier">
	                <type>UUID</type>
	            </prop>
	            <prop key="http://www.disit.org/km4city/schema#lastChargeTime">
	                <type>Float</type>
	                <maxValue>${chargeState1} + 5</maxValue>
	                <minValue>2</minValue>
	                <uri>http://www.w3.org/2001/XMLSchema#float</uri>
	            </prop>
			</properties>
		</class>
    </iterationElement>
</tree>